{
	"env": {
		"node": true
	},
	"extends": ["airbnb", "prettier", "plugin:node/recommended", "plugin:security/recommended"],
	"plugins": ["prettier", "security"],
	"parserOptions": { "ecmaVersion": "latest" },
	"rules": {
		"prettier/prettier": "error",
		"func-names": "off",
		"no-process-exit": "off",
		"no-unused-vars": "warn",
		"object-shorthand": "off"
	}
}
